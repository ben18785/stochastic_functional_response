Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"meanNumberEaten", "[", 
    RowBox[{"N0_", ",", "a_", ",", "r_", ",", "tval_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"vars", ",", "eqns", ",", "init", ",", "sol", ",", "meanExpr"}],
       "}"}], ",", 
     RowBox[{"(*", 
      RowBox[{"Define", " ", "state", " ", "variables"}], "*)"}], 
     RowBox[{
      RowBox[{"vars", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{"P", "[", 
              RowBox[{"N", ",", "1"}], "]"}], "[", "t", "]"}], ",", 
            RowBox[{
             RowBox[{"P", "[", 
              RowBox[{"N", ",", "0"}], "]"}], "[", "t", "]"}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"N", ",", "0", ",", "N0"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "ODE", " ", "system", " ", "with", " ", "corrected", " ", 
        "transitions"}], "*)"}], 
      RowBox[{"eqns", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"P", "[", 
                RowBox[{"N", ",", "1"}], "]"}], "'"}], "[", "t", "]"}], "==", 
             
             RowBox[{
              RowBox[{
               RowBox[{"-", "a"}], "*", "N", "*", 
               RowBox[{
                RowBox[{"P", "[", 
                 RowBox[{"N", ",", "1"}], "]"}], "[", "t", "]"}]}], "+", 
              RowBox[{"r", "*", 
               RowBox[{
                RowBox[{"P", "[", 
                 RowBox[{"N", ",", "0"}], "]"}], "[", "t", "]"}]}]}]}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"P", "[", 
                RowBox[{"N", ",", "0"}], "]"}], "'"}], "[", "t", "]"}], "==", 
             
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"N", "+", "1"}], "<=", "N0"}], ",", 
                RowBox[{"a", "*", 
                 RowBox[{"(", 
                  RowBox[{"N", "+", "1"}], ")"}], "*", 
                 RowBox[{
                  RowBox[{"P", "[", 
                   RowBox[{
                    RowBox[{"N", "+", "1"}], ",", "1"}], "]"}], "[", "t", 
                  "]"}]}], ",", "0"}], "]"}], "-", 
              RowBox[{"r", "*", 
               RowBox[{
                RowBox[{"P", "[", 
                 RowBox[{"N", ",", "0"}], "]"}], "[", "t", "]"}]}]}]}]}], 
           "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"N", ",", "0", ",", "N0"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Initial", " ", "conditions"}], "*)"}], 
      RowBox[{"init", "=", 
       RowBox[{"Flatten", "[", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"If", "[", 
             RowBox[{
              RowBox[{"N", "==", "N0"}], ",", 
              RowBox[{
               RowBox[{
                RowBox[{"P", "[", 
                 RowBox[{"N", ",", "1"}], "]"}], "[", "0", "]"}], "==", "1"}],
               ",", 
              RowBox[{
               RowBox[{
                RowBox[{"P", "[", 
                 RowBox[{"N", ",", "1"}], "]"}], "[", "0", "]"}], "==", 
               "0"}]}], "]"}], ",", 
            RowBox[{
             RowBox[{
              RowBox[{"P", "[", 
               RowBox[{"N", ",", "0"}], "]"}], "[", "0", "]"}], "==", "0"}]}],
            "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"N", ",", "0", ",", "N0"}], "}"}]}], "]"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Solve", " ", "the", " ", "ODEs", " ", "numerically"}], "*)"}], 
      RowBox[{"sol", "=", 
       RowBox[{"NDSolveValue", "[", 
        RowBox[{
         RowBox[{"Join", "[", 
          RowBox[{"eqns", ",", "init"}], "]"}], ",", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{"P", "[", 
               RowBox[{"N", ",", "1"}], "]"}], "[", "t", "]"}], ",", 
             RowBox[{
              RowBox[{"P", "[", 
               RowBox[{"N", ",", "0"}], "]"}], "[", "t", "]"}]}], "}"}], ",", 
           
           RowBox[{"{", 
            RowBox[{"N", ",", "0", ",", "N0"}], "}"}]}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"t", ",", "0", ",", "tval"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Compute", " ", "mean", " ", "at", " ", "time", " ", "tval", " ", 
        "using", " ", "evaluated", " ", "solution"}], "*)"}], 
      RowBox[{"meanExpr", "=", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{"N", "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"sol", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{"N", "+", "1"}], ",", "1"}], "]"}], "]"}], "+", 
             RowBox[{"sol", "[", 
              RowBox[{"[", 
               RowBox[{
                RowBox[{"N", "+", "1"}], ",", "2"}], "]"}], "]"}]}], "/.", 
            " ", 
            RowBox[{"t", "->", "tval"}]}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"N", ",", "0", ",", "N0"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"N0", "-", "meanExpr"}]}]}], "]"}]}], "\n"}], "\n", 
 RowBox[{"Flatten", "[", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"{", 
